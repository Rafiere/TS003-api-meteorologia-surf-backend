{
  "name": "back-end",
  "version": "1.0.0",
  "description": "API de Metereologia para Surfistas - NodeJS",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:local": "ts-node-dev --files 'src/index.ts' | pino-pretty -c",
    "test": "yarn test:unit && yarn test:functional",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'"
  },
  "keywords": [],
  "author": "Rafael",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rafiere/TS003-api-meteorologia-surf.git"
  },
  "bugs": {
    "url": "https://github.com/Rafiere/TS003-api-meteorologia-surf/issues"
  },
  "homepage": "https://github.com/Rafiere/TS003-api-meteorologia-surf#readme",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^0.0.41",
    "@types/jest": "^27.0.3",
    "@types/module-alias": "^2.0.1",
    "@types/mongoose": "^5.11.97",
    "@types/nock": "^11.1.0",
    "@types/node": "16",
    "@types/pino": "^7.0.5",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.5.0",
    "jest": "^27.4.5",
    "nock": "^12.0.3",
    "pino-pretty": "^8.1.0",
    "prettier": "^2.7.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "config": "^3.3.7",
